{
  "expo": {
    "name": "RickAndMortyApp",
    "slug": "RickAndMortyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/rick-morty-pixel-art.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/rick-morty-pixel-art.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabrielrpimenta.RickAndMortyApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gabrielrpimenta.RickAndMortyApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
